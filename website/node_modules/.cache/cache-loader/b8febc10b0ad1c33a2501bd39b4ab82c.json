en: []\n}, {\n  value: 'Examples',\n  id: 'examples',\n  children: [{\n    value: 'Generate',\n    id: 'generate',\n    children: []\n  }, {\n    value: 'When',\n    id: 'when',\n    children: []\n  }]\n}];\nexport const metadata = {\n  \"id\": \"vhdp_structuralsyntax_vhdl\",\n  \"title\": \"VHDL\",\n  \"description\": \"## Definition\\r\",\n  \"source\": \"@site/..\\\\docs\\\\vhdp_structuralsyntax_vhdl.md\",\n  \"permalink\": \"/docs/vhdp_structuralsyntax_vhdl\",\n  \"lastUpdatedBy\": \"Author\",\n  \"lastUpdatedAt\": 1539502055,\n  \"sidebar_label\": \"VHDL\",\n  \"sidebar\": \"docs\",\n  \"previous\": {\n    \"title\": \"Connections\",\n    \"permalink\": \"/docs/vhdp_structuralsyntax_connections\"\n  },\n  \"next\": {\n    \"title\": \"If, Else and Elsif\",\n    \"permalink\": \"/docs/vhdp_standardoperation_if\"\n  }\n};\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc,\nmetadata\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <h2 {...{\n      \"id\": \"definition\"\n    }}>{`Definition`}</h2>\n    <p>{`For all functions that are not (already) implemented, with `}<inlineCode parentName=\"p\">{`VHDL{}`}</inlineCode>{` you can add VHDL code to your VHDP code. If you\nwant to add something to the signal declaration area, you have to write `}<inlineCode parentName=\"p\">{`AttributeDeclaration{VHDL{}}`}</inlineCode>{`.`}</p>\n    <h2 {...{\n      \"id\": \"examples\"\n    }}>{`Examples`}</h2>\n    <h3 {...{\n      \"id\": \"generate\"\n    }}>{`Generate`}</h3>\n    <pre><code parentName=\"pre\" {...{\n        \"className\": \"language-vhdp\"\n      }}>{`AttributeDeclaration\n{\n    VHDL\n    {\n        COMPONENT BinaryToBcdDigit IS \n        PORT(\n            CLK : IN STD_LOGIC;\n            Reset : IN STD_LOGIC;\n            ena : IN STD_LOGIC;\n            binary : IN STD_LOGIC;\n            c_out : BUFFER STD_LOGIC;\n            bcd : BUFFER STD_LOGIC_VECTOR(3 DOWNTO 0)\n        );\n        END COMPONENT;\n    }\n}\nVHDL\n{\n    bcd_digits: FOR i IN 1 TO 5 GENERATE\n        digit_0: BinaryToBcdDigit PORT MAP \n        (\n            CLK,\n            Reset,\n            converter_ena,\n            converter_inputs(i-1),\n            converter_inputs(i),\n            bcd_reg(i*4-1 DOWNTO i*4-4)\n        );\n    END GENERATE;\n}\n`}</code></pre>\n    <h3 {...{\n      \"id\": \"when\"\n    }}>{`When`}</h3>\n    <pre><code parentName=\"pre\" {...{\n        \"className\": \"language-vhdp\"\n      }}>{`VHDL\n{\n    SDA <= '0' WHEN sda_ena_n = '0' ELSE 'Z';\n}\n`}</code></pre>\n\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}