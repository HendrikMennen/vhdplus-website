cAEF,MAAO,MAAMC,CAAAA,QAAQ,CAAG,EAAjB,CACP,MAAO,MAAMC,CAAAA,QAAQ,CAAG,CACtB,KAAM,aADgB,CAEtB,QAAS,0BAFa,CAGtB,cAAe,+HAHO,CAItB,SAAU,gCAJY,CAKtB,YAAa,mBALS,CAMtB,gBAAiB,QANK,CAOtB,gBAAiB,UAPK,CAQtB,gBAAiB,SARK,CAStB,UAAW,MATW,CAUtB,WAAY,CACV,QAAS,uBADC,CAEV,YAAa,2BAFH,CAVU,CActB,OAAQ,CACN,QAAS,iBADH,CAEN,YAAa,6BAFP,CAdc,CAAjB,CAmBP,KAAMC,CAAAA,aAAa,CAAGC,IAAI,EAAI,QAASC,CAAAA,mBAAT,CAA6BC,KAA7B,CAAoC,CAChEC,OAAO,CAACC,IAAR,CAAa,aAAeJ,IAAf,CAAsB,yEAAnC,EACA,MAAO,WAASE,KAAT,CAAP,CACD,CAHD,CAKA,KAAMG,CAAAA,WAAW,CAAG,CAClBR,QADkB,CAEpBC,QAFoB,CAApB,CAIA,KAAMQ,CAAAA,SAAS,CAAG,SAAlB,CACA,cAAe,SAASC,CAAAA,UAAT,CAAoB,CACjCC,UADiC,CAEjC,GAAGN,KAF8B,CAApB,CAGZ,CACD,MAAO,KAAC,SAAD,aAAeG,WAAf,CAAgCH,KAAhC,EAAuC,UAAU,CAAEM,UAAnD,CAA+D,OAAO,CAAC,WAAvE,GAEL,aAAK;;;kIAAL,CAFK,CAML,aAAK;iEAAL,CAC8D,SAAG,IAAI,CAAC,uCAAR,CAAgD,MAAM,CAAC,QAAvD,EAAkE,WAAlE,CAD9D,CANK,CAQL,aAAG,oBAAK,UAAU,CAAC,GAAhB,EAAwB,CACvB,MAAO,iCADgB,CAEvB,MAAO,eAFgB,CAAxB,EAAH,CARK,CAYL,aAAK,oGAAL,CAZK,CAaL,WAAK,KAAK,CAAC,YAAX,EAAwB,cAAQ,EAAE,CAAC,UAAX,CAAsB,IAAI,CAAC,WAA3B,CAAuC,KAAK,CAAC,MAA7C,CAAoD,GAAG,CAAC,gFAAxD,CAAyI,WAAW,CAAC,GAArJ,CAAyJ,eAAe,KAAxK,EAAxB,CAbK,CAAP,CAgBD,CAED,CACAD,UAAU,CAACE,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\tid: 'guide_nios2',\n\ttitle: 'Programming with Arduino',\n\tsidebar_label: 'NIOS II'\n};\n  /* @jsx mdx */\n\nexport const rightToc = [];\nexport const metadata = {\n  \"id\": \"guide_nios2\",\n  \"title\": \"Programming with Arduino\",\n  \"description\": \"It is possible to design a processor with the logic elements of the FPGA. This is for Intel a Nios II soft core processor. \\r\",\n  \"source\": \"@site/..\\\\docs\\\\guide_nios2.md\",\n  \"permalink\": \"/docs/guide_nios2\",\n  \"lastUpdatedBy\": \"Author\",\n  \"lastUpdatedAt\": 1539502055,\n  \"sidebar_label\": \"NIOS II\",\n  \"sidebar\": \"docs\",\n  \"previous\": {\n    \"title\": \"Simulate your program\",\n    \"permalink\": \"/docs/getstarted_modelsim\"\n  },\n  \"next\": {\n    \"title\": \"Code Completion\",\n    \"permalink\": \"/docs/ide_editor_completion\"\n  }\n};\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc,\nmetadata\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <p>{`It is possible to design a processor with the logic elements of the FPGA. This is for Intel a Nios II soft core processor.\nAfter loading the processor on the FPGA, you can program it with the Nios II Software Build Tools for Eclipse (Tools/Program NIOS II).\nThe processor can be used like a 32-Bit microcontroller, but you can configure it according to your needs. Just add more I/Os or another SPI interface if you need them!\nOf course you can still use the left over logic elements and RAM together with the processor to combine FPGA and microcontroller.`}</p>\n    <p>{`Because this processor can be programmed with c++, people made the Arduino libraries compatible with the Nios II processor.\nCheck out the NIOSDuino project on GitHub for more information: `}<a href=\"https://github.com/dimag0g/nios_duino\" target=\"_blank\">{`NIOSDuino`}</a></p>\n    <p><img parentName=\"p\" {...{\n        \"src\": \"assets/getstarted/NIOSDuino.PNG\",\n        \"alt\": \"VHDP Overview\"\n      }}></img></p>\n    <p>{`Watch this tutorial to see how to add a processor to your FPGA design and program it with Arduino:`}</p>\n    <div class=\"fluidMedia\"><iframe id=\"ytplayer\" type=\"text/html\" width=\"100%\" src=\"https://www.youtube.com/embed/YhavjKajX_w?autoplay=0&origin=http://vhdplus.com\" frameborder=\"0\" allowfullscreen></iframe></div>\n\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}