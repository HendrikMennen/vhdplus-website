'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\ttitle: 'IDE Light Theme',\n\tauthor: 'Hendrik Mennen',\n\tauthorURL: 'http://twitter.com/hendrikmennen',\n\tauthor_image_url: 'https://avatars0.githubusercontent.com/u/25281882?s=460&v=4'\n};\n  /* @jsx mdx */\n\nexport const rightToc = [{\n  value: 'Light Theme',\n  id: 'light-theme',\n  children: [{\n    value: 'How to use it',\n    id: 'how-to-use-it',\n    children: []\n  }]\n}, {\n  value: 'Future plans',\n  id: 'future-plans',\n  children: []\n}];\nexport const metadata = {\n  \"permalink\": \"/blog/2019/10/17/blog.post\",\n  \"source\": \"@site/blog\\\\2019-10-17-blog.post.md\",\n  \"description\": \"Not everyone likes the dark theme that is selected as the default theme for VHDPlus IDE.\\r\",\n  \"date\": \"2019-10-17T00:00:00.000Z\",\n  \"tags\": [],\n  \"title\": \"IDE Light Theme\",\n  \"prevItem\": {\n    \"title\": \"Serial Monitor\",\n    \"permalink\": \"/blog/2019/10/22/blog-post\"\n  },\n  \"nextItem\": {\n    \"title\": \"Compiler 2.0\",\n    \"permalink\": \"/blog/2019/10/01/blog-post\"\n  }\n};\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc,\nmetadata\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <p>{`Not everyone likes the dark theme that is selected as the default theme for VHDPlus IDE.\nUpdate 0.9.2.6 gives you the ability to chose a alternative theme with brighter colors.`}</p>\n    {\n      /*truncate*/\n    }\n    <h2 {...{\n      \"id\": \"light-theme\"\n    }}>{`Light Theme`}</h2>\n    <p><img parentName=\"p\" {...{\n        \"src\": \"/img/blog/2019-10-17/LightMode.png\",\n        \"alt\": \"Light theme\"\n      }}></img></p>\n    <h3 {...{\n      \"id\": \"how-to-use-it\"\n    }}>{`How to use it`}</h3>\n    <ol>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`Navigate to `}<inlineCode parentName=\"p\">{`Extras`}</inlineCode>{` -> `}<inlineCode parentName=\"p\">{`Settings`}</inlineCode></p>\n      </li>\n      <li parentName=\"ol\">\n        <p parentName=\"li\">{`In the `}<inlineCode parentName=\"p\">{`General`}</inlineCode>{` category, go to `}<inlineCode parentName=\"p\">{`Theme`}</inlineCode>{` and select `}<inlineCode parentName=\"p\">{`Light`}</inlineCode></p>\n      </li>\n    </ol>\n    <p><img parentName=\"p\" {...{\n        \"src\": \"/img/blog/2019-10-17/LightModeSelect.PNG\",\n        \"alt\": \"Light theme select\"\n      }}></img></p>\n    <h2 {...{\n      \"id\": \"future-plans\"\n    }}>{`Future plans`}</h2>\n    <ul>\n      <li parentName=\"ul\">{`Implement a cross-platform ClearType to improve readability.`}</li>\n      <li parentName=\"ul\">{`Support custom themes`}</li>\n    </ul>\n\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}