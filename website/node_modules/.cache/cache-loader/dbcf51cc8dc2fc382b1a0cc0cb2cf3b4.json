V,QAD/V,CACuW,kBAAY,UAAU,CAAC,GAAvB,EAA6B,QAA7B,CADvW,CAC2Z,gBAD3Z,CA3BK,CA6BL,SAAQ,CACN,KAAM,WADA,CAAR,CAEK,SAFL,CA7BK,CAgCL,eAAK,qBAAM,UAAU,CAAC,KAAjB,EAA2B,CAC5B,YAAa,eADe,CAA3B,EAEE;;;;;;;;;CAFF,CAAL,CAhCK,CAAP,CA8CD,CAED,CACAD,UAAU,CAACE,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\tid: 'vhdp_procedureoperation_while',\n\ttitle: 'While',\n\tsidebar_label: 'While'\n};\n  /* @jsx mdx */\n\nexport const rightToc = [{\n  value: 'Definition',\n  id: 'definition',\n  children: []\n}, {\n  value: 'Example',\n  id: 'example',\n  children: []\n}, {\n  value: 'Without Thread',\n  id: 'without-thread',\n  children: []\n}, {\n  value: 'Example',\n  id: 'example-1',\n  children: []\n}];\nexport const metadata = {\n  \"id\": \"vhdp_procedureoperation_while\",\n  \"title\": \"While\",\n  \"description\": \"## Definition\\r\",\n  \"source\": \"@site/..\\\\docs\\\\vhdp_procedureoperation_while.md\",\n  \"permalink\": \"/docs/vhdp_procedureoperation_while\",\n  \"lastUpdatedBy\": \"Author\",\n  \"lastUpdatedAt\": 1539502055,\n  \"sidebar_label\": \"While\",\n  \"sidebar\": \"docs\",\n  \"previous\": {\n    \"title\": \"StepFor\",\n    \"permalink\": \"/docs/vhdp_procedureoperation_stepfor\"\n  },\n  \"next\": {\n    \"title\": \"Wait\",\n    \"permalink\": \"/docs/vhdp_procedureoperation_wait\"\n  }\n};\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc,\nmetadata\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <h2 {...{\n      \"id\": \"definition\"\n    }}>{`Definition`}</h2>\n    <p>{`With this section you can repeat operations as long as a condition is met.`}</p>\n    <h2 {...{\n      \"id\": \"example\"\n    }}>{`Example`}</h2>\n    <pre><code parentName=\"pre\" {...{\n        \"className\": \"language-vhdp\"\n      }}>{`Thread \n{ \n    VARIABLE counter : INTEGER := 0; \n    While (counter < 8) \n    { \n        LED(counter) <= '1'; \n        counter := counter + 1; \n        Wait(1000); \n    } \n}\n`}</code></pre>\n    <p>{`In this example, `}<inlineCode parentName=\"p\">{`counter`}</inlineCode>{` is counted form 0 to 7. The counter'th position of the LED vector is set high and between the loops, 1000 cycles are waited.\n`}<strong parentName=\"p\">{`Important:`}</strong>{` This loop won't repeat counting to 7, because the default value of the variable is only set at the verry beginning.`}</p>\n    <h2 {...{\n      \"id\": \"without-thread\"\n    }}>{`Without Thread`}</h2>\n    <p>{`If you use `}<inlineCode parentName=\"p\">{`While`}</inlineCode>{` outside of a `}<inlineCode parentName=\"p\">{`Thread`}</inlineCode>{`, the VHDL `}<inlineCode parentName=\"p\">{`While`}</inlineCode>{` is used. In that section you can repeat only a finite amount of times.\nThe `}<inlineCode parentName=\"p\">{`While`}</inlineCode>{` in a thread is called `}<inlineCode parentName=\"p\">{`StepWhile`}</inlineCode>{` like `}<inlineCode parentName=\"p\">{`StepFor`}</inlineCode>{`, but because of the small use of the VHDL `}<inlineCode parentName=\"p\">{`While`}</inlineCode>{`, every `}<inlineCode parentName=\"p\">{`While`}</inlineCode>{` in a `}<inlineCode parentName=\"p\">{`Thread`}</inlineCode>{` is converted.`}</p>\n    <h2 {...{\n      \"id\": \"example-1\"\n    }}>{`Example`}</h2>\n    <pre><code parentName=\"pre\" {...{\n        \"className\": \"language-vhdp\"\n      }}>{`Function Eight (return INTEGER; i : NATURAL)\n{ \n    VARIABLE counter : INTEGER := i; \n    While (counter < 8) \n    { \n        counter := counter + 1; \n    } \n    return counter;\n}\n`}</code></pre>\n\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}